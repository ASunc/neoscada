<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.eclipse.scada.ide.hd.hdsspy.hds"
            file-extensions="hds">
      </file-association>
      <content-type
            file-extensions="hds"
            id="org.eclipse.scada.ide.hd.hdsspy.hds"
            name="Eclipse SCADA Value Archive File"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.scada.ide.hd.hdsspy.editors.HDSEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="hds"
            icon="icons/content-hds.png"
            id="org.eclipse.scada.ide.hd.hdsspy.editors.HDSEditor"
            name="Eclipse SCADA Value Archive Editor">
         <contentTypeBinding
               contentTypeId="org.eclipse.scada.ide.hd.hdsspy.hds">
         </contentTypeBinding>
      </editor>
   </extension>

</plugin>
